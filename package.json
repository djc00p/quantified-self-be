{
  "name": "quantified-self-be",
  "version": "1.0.0",
  "description": "A calorie tracker project for Mod4 BE at Turing School of Software and Design",
  "main": "app.js",
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest.config"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules"
    ]
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.17.1",
    "morgan": "~1.9.1",
    "node-sass-middleware": "0.11.0",
    "pg": "^7.11.0",
    "sequelize": "^5.9.2",
    "sequelize-cli": "^5.5.0"
  },
  "devDependencies": {
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "supertest": "^4.0.2"
  },
  "scripts": {
    "start": "node bin/www",
    "pretest": "NODE_ENV=test npx sequelize db:create && NODE_ENV=test npx sequelize db:migrate",
    "test": "jest",
    "posttest": "NODE_ENV=test npx sequelize db:drop",
    "debug-test": "node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joequincy/quantified-self-be.git"
  },
  "author": "Jon Peterson, Deonte Cooper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joequincy/quantified-self-be/issues"
  },
  "homepage": "https://github.com/joequincy/quantified-self-be#readme"
}
